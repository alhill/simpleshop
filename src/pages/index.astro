---
import Layout from '../layouts/Layout.astro'
import addRoot from '../utils/addRoot'

let product = { loading: true }

try{
	const productRes = await fetch(addRoot("/api/get-products"), {
		method: "POST",
		headers: {
			"Accept": "application/json",
			"Content-Type": "application/json"
		}
	})
	product = await productRes?.json()
} catch(err) {
	console.error(err)
}

---

<Layout>
	<!-- <Welcome /> -->
	{ JSON.stringify(product) }
</Layout>
